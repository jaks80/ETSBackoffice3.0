                        <storetype>JKS</storetype>
                        <alias>033644e3-12bc-11e5-bed4-b8763fad0657</alias>
                        <verify>true</verify> 
                    </sign>
                    <pack200>false</pack200>
                    <gzip>true</gzip>
                    <outputJarVersions>false</outputJarVersions>
                    <verbose>true</verbose>
                </configuration>
            </plugin>
        
